{
  "name": "middleware-supertest",
  "description": "Testing Express.js RequestHandler middlewares both on server-side and client-side",
  "version": "0.8.9",
  "author": "Yusuke Kawasaki <u-suke@kawa.net>",
  "contributors": [
    "Yusuke Kawasaki <u-suke@kawa.net>"
  ],
  "dependencies": {
    "express": "*",
    "express-intercept": "^0.8.9",
    "supertest": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.44",
    "@types/supertest": "^2.0.11",
    "mocha": "^8.3.2",
    "typescript": "^4.2.4"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/*.js",
    "types/*.d.ts"
  ],
  "homepage": "https://github.com/kawanet/middleware-supertest#readme",
  "keywords": [
    "assert",
    "expressjs",
    "middleware",
    "supertest",
    "test"
  ],
  "license": "MIT",
  "main": "lib/middleware-supertest.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/middleware-supertest.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -p . && ./node_modules/.bin/tsc types/*.d.ts",
    "fixpack": "fixpack",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "test": "npm run mocha",
    "tsc-watch": "tsc --watch"
  },
  "typings": "./types/middleware-supertest.d.ts"
}
