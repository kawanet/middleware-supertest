{
  "name": "middleware-supertest",
  "description": "Testing Express.js RequestHandler middlewares both on server-side and client-side",
  "version": "0.8.1",
  "author": "Yusuke Kawasaki <u-suke@kawa.net>",
  "contributors": [
    "Yusuke Kawasaki <u-suke@kawa.net>"
  ],
  "dependencies": {
    "express": "*",
    "express-intercept": "^0.8.0",
    "supertest": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "@types/supertest": "^2.0.10",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/*.js",
    "types/*.d.ts"
  ],
  "homepage": "https://github.com/kawanet/middleware-supertest#readme",
  "keywords": [
    "assert",
    "expressjs",
    "middleware",
    "supertest",
    "test"
  ],
  "license": "MIT",
  "main": "lib/middleware-supertest.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/middleware-supertest.git"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-types",
    "build-lib": "./node_modules/.bin/tsc -p .",
    "build-types": "./node_modules/.bin/tsc -p types && perl -i -pe 's/^ *private .*$//' types/*.d.ts",
    "fixpack": "fixpack",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "test": "npm run mocha",
    "tsc-watch": "tsc --watch"
  },
  "typings": "./types/middleware-supertest.d.ts"
}
